<!-- This is the LGE IMS features. -->
<permissions>
    <!-- IMSPhone - for interworking with RIL (modem) -->
    <feature name="com.lge.ims.imsphone" />
    <!-- VoLTE -->
    <feature name="com.lge.ims.volte" />
    <feature name="com.lge.software.volte" />
    <feature name="com.lge.ims.srvcc" />
    <!-- UCE -->
    <feature name="com.lge.ims.service.eab" />
    <!-- Video Call -->
    <feature name="com.lge.ims.service.videocall" />
    <feature name="com.lge.ims.vt" />
</permissions>
